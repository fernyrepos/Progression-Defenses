<?xml version="1.0" encoding="utf-8" ?>          
<Patch>       
                <Operation Class="PatchOperationFindMod">
                  <mods>
                    <li>Medieval Overhaul</li>
                  </mods>
                  <match Class="PatchOperationSequence">
                    <operations>
                        <li Class="PatchOperationAdd">
                            <xpath>Defs</xpath>
                            <value>
                                <VEF.Buildings.HiddenDesignatorsDef>
                                    <defName>FernyMOVFESecurityProgWarfare_HiddenDesignators</defName>
                                    <hiddenDesignators>			
                                        <li>VFES_CavalrySpikes</li>
                                        <li>DankPyon_RTrench</li>
                                        <li>DankPyon_Trench</li>
                                    </hiddenDesignators>							
                                </VEF.Buildings.HiddenDesignatorsDef>
                            </value>
                        </li>
                        <li Class="PatchOperationReplace">
                            <xpath>Defs/ResearchProjectDef[defName="VWE_TrenchWarfare"]/label</xpath>
                            <value>
                            <label>Advanced tactics</label>
                            </value>
                        </li>
                        <li Class="PatchOperationReplace">
                            <xpath>Defs/ResearchProjectDef[defName="VWE_TrenchWarfare"]/description</xpath>
                            <value>
                            <description>Adapt advanced battlefield tactics with a selection of weapons for close-quarters combat.</description>
                            </value>
                        </li>

                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName = "VFES_Turret_Ballista" or defName="VFES_Turret_Catapult" or defName="VFES_CavalrySpikes"]/building/buildingTags</xpath>
                        </li>
                        
                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName = "VFES_Turret_Ballista" or defName="VFES_Turret_Catapult" or defName="VFES_CavalrySpikes"]/designationCategory</xpath>
                        </li>

                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName = "VFES_Turret_Ballista" or defName="VFES_Turret_Catapult" or defName="VFES_CavalrySpikes"]/researchPrerequisites</xpath>
                        </li>

                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ResearchProjectDef[defName="VFES_SiegeEquipment"]</xpath>
                        </li>

                    </operations>
                  </match>
                </Operation>
    




</Patch>