<?xml version="1.0" encoding="utf-8" ?>          
<Patch>       
                <Operation Class="PatchOperationFindMod">
                  <mods>
                    <li>Medieval Overhaul</li>
                  </mods>
                  <match Class="PatchOperationSequence">
                    <operations>
                        <li Class="PatchOperationReplace">
                            <xpath>Defs/ThingDef[defName="VFES_CavalrySpikes"]/designationCategory</xpath>
                            <value>
                            <designationCategory></designationCategory>
                            </value>
                        </li>
                        <li Class="PatchOperationReplace">
                            <xpath>Defs/ThingDef[defName="VFES_Trench"]/designationCategory</xpath>
                            <value>
                            <designationCategory></designationCategory>
                            </value>
                        </li>
                        <li Class="PatchOperationReplace">
                            <xpath>Defs/ResearchProjectDef[defName="VWE_TrenchWarfare"]/label</xpath>
                            <value>
                            <label>Advanced tactics</label>
                            </value>
                        </li>
                        <li Class="PatchOperationReplace">
                            <xpath>Defs/ResearchProjectDef[defName="VWE_TrenchWarfare"]/description</xpath>
                            <value>
                            <description>Adapt advanced battlefield tactics with a selection of weapons for close-quarters combat.</description>
                            </value>
                        </li>
                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName="VFES_Trench"]/researchPrerequisites</xpath>
                        </li>

		  
                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName = "VFES_Turret_Ballista" or defName="VFES_Turret_Catapult"]/building/buildingTags</xpath>
                        </li>
                        
                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName = "VFES_Turret_Ballista" or defName="VFES_Turret_Catapult"]/designationCategory</xpath>
                        </li>

                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ThingDef[defName = "VFES_Turret_Ballista" or defName="VFES_Turret_Catapult"]/researchPrerequisites</xpath>
                        </li>

                        <li Class="PatchOperationRemove">
                            <xpath>Defs/ResearchProjectDef[defName = "VFES_SiegeEquipment"]</xpath>
                        </li>

                    </operations>
                  </match>
                </Operation>
</Patch>